<table class="member" id="member-{{characterId}}">
  <tr>
    <td class="ship-icon">{{{shipIcon}}}</td>
    <td class="name"><a href="javascript:CCPEVE.showInfo(1377, {{characterId}});">{{characterName}}</a></td>
    <td class="system {{#if isDocked}}docked{{/if}}"><a href="javascript:CCPEVE.showInfo(5,{{systemId}});">{{systemName}}</a></td>
    <td class="actions">
      <a title="Set Destination" onClick="CCPEVE.setDestination({{systemId}})"><i class="fa fa-map-marker" title="Set Destination"></i></a>
      <a title="Start Conversation" onClick="CCPEVE.startConversation({{characterId}})"><i class="fa fa-comment" title="Start Conversation"></i></a>
      <a title="Killboard: {{characterName}}" target="_blank" href="https://zkillboard.com/character/{{characterName}}/"><i class="fa fa-crosshairs" title="Killboard"></i></a>
    </td>
  </tr>
</table>
